{
  "name": "josemi-anton-resume",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rm -rf dist .astro .vite-cache",
    "type-check": "astro check && tsc --noEmit",
    "lint": "astro check",
    "deploy": "pnpm build && npx wrangler deploy",
    "assets:sync": "export $(grep -v '^#' .env | xargs) && node lib/assets/sync.js",
    "assets:status": "export $(grep -v '^#' .env | xargs) && node lib/assets/status.js",
    "images:add": "./scripts/add-image.sh"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.8",
    "@astrojs/react": "^4.3.1",
    "@astrojs/sitemap": "^3.5.1",
    "@aws-sdk/client-s3": "^3.888.0",
    "@icons-pack/react-simple-icons": "^10.2.0",
    "@radix-ui/react-slot": "^1.2.3",
    "astro": "^5.13.7",
    "astro-icon": "^1.1.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.456.0",
    "mime-types": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^6.0.2",
