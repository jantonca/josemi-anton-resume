# .env.example - Copy to .env and fill in your values

# Cloudflare Account ID (from dashboard)
CF_ACCOUNT_ID=your-account-id-here

# R2 API Credentials
# Get these from: Dashboard > R2 > Manage R2 API Tokens > Create API Token
R2_ACCESS_KEY_ID=your-access-key-id
R2_SECRET_ACCESS_KEY=your-secret-access-key

# Upload endpoint secret (optional - for API uploads)
UPLOAD_SECRET=your-secure-upload-secret-here

# Environment
NODE_ENV=development